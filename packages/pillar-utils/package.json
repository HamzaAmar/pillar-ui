{
  "name": "@pillar/utils",
  "version": "0.0.0",
  "sideEffects": false,
  "description": "A collection of utility functions and tools to assist with common tasks and improve the efficiency of web development. These utilities are carefully crafted to enhance productivity and streamline the development process.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "npx tsup",
    "build:watch": "npx tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "yarn build:watch",
    "test": "vitest --watch"
  },
  "devDependencies": {
    "@pillar/tsconfig": "*",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "eslint-config-custom": "*",
    "react": "^18.1.0",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3",
    "vitest": "^0.31.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
