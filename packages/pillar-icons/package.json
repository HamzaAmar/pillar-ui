{
  "name": "@pillar/icons",
  "version": "0.0.0",
  "description": "A library of scalable icons suitable for various web development projects. These icons are highly customizable, accessible, and designed to seamlessly integrate into modern user interfaces.",
  "keywords": [
    "icons",
    "icon library",
    "scalable icons",
    "web development",
    "user interface",
    "customizable",
    "accessibility",
    "modern",
    "vector",
    "@pillar-icons",
    "development",
    "components",
    "SVG",
    "visual",
    "web design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HamzaAmar/pillar-ui",
    "directory": "packages/pillar-icons"
  },
  "bugs": "https://github.com/HamzaAmar/pillar-ui/issues",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "author": {
    "name": "Hamza Miloud Amar",
    "email": "hamzamiloudamar@gmail.com"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./arrows": "./dist/icons/arrows/index.js",
    "./action": "./dist/icons/action/index.js",
    "./brand": "./dist/icons/brand/index.js",
    "./building": "./dist/icons/building/index.js",
    "./currency": "./dist/icons/currency/index.js",
    "./food": "./dist/icons/food/index.js",
    "./hand": "./dist/icons/hand/index.js",
    "./mail": "./dist/icons/mail/index.js",
    "./media": "./dist/icons/media/index.js",
    "./navigation": "./dist/icons/navigation/index.js",
    "./number": "./dist/icons/number/index.js",
    "./science": "./dist/icons/science/index.js",
    "./social": "./dist/icons/social/index.js",
    "./sport": "./dist/icons/sport/index.js",
    "./transportation": "./dist/icons/transportation/index.js",
    "./typography": "./dist/icons/typography/index.js",
    "./vowel": "./dist/icons/vowel/index.js",
    "./weather": "./dist/icons/weather/index.js"
  },
  "scripts": {
    "build": "npx tsup",
    "build:watch": "npx tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "yarn build:watch",
    "type:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@pillar/tsconfig": "*",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "eslint-config-pillar": "*",
    "react": "^18.1.0",
    "tsup": "^6.7.0",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
