{
  "name": "@pillar/icons",
  "version": "0.0.0",
  "description": "pillar icons is an icons package for use in modern web development projects. This package provides a comprehensive set of high-quality, scalable icons that are fully customizable and easy to use in your projects.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "keywords": [
    "react",
    "typescript",
    "icons",
    "svg",
    "scalable",
    "pillar-icons",
    "design-system",
    "accessible"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@pillar/tsconfig": "*",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "eslint-config-pillar": "*",
    "react": "^18.1.0",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@stitches/react": "^1.2.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
