    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reading-time": "^1.5.0",
    "sugar-high": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@pillar-ui/eslint": "*",
    "@pillar-ui/tsconfig": "*",
